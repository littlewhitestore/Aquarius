.list {
  display: flex;
  flex-direction: column;
  padding: 10rpx 8rpx;
  font-size: 14px;
}

.num {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.no {
  color: #818181;
}

.status {
  color: rgba(254, 110, 31, 0.87);
}

.line {
  border: solid 1rpx #e1dfdf;
  margin: 10rpx 0;
  position: relative;
}
.liner{
border: solid 1rpx #e1dfdf;
}

.li {
  position: absolute;
  left: 10rpx;
  top: -2rpx;
  width: 300rpx;
  border: solid 3rpx #ff9e9e;
}

.con {
  padding-top: 10rpx;
  height: 210rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.img {
  height: 200rpx;
  width: 200rpx;
}

.font {
  display: flex;
  flex-direction: column;
  width: 480rpx;
}

.name {
  font-size: 16px;
  color: rgba(129, 129, 129, 0.87);
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.info {
  margin: 10rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 12px;
  color: #757575;
}

.price {
  color: #fe6e1f;
  font-size: 16px;
}
.bottom{
  margin: 20rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 15px;
  color: #48434c
}
.share image{
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}
.btn{
  padding: 0 6rpx;
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
    background-color: #fe6e1f;
    color: #fff;
    font-size: 14px;
}
.space{
  height: 20rpx;
    background-color: #f7f7f7;
}
